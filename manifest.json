{
    "name": "LoL Current Song",
    "version": "1.0.0",
    "description": "Extension for LoL Current Song",
    "manifest_version": 2,
    "permissions": [
        "nativeMessaging",
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "icons": {
        "16": "images/logo-16.png",
        "32": "images/logo-32.png",
        "48": "images/logo-48.png",
        "128": "images/logo-128.png"
    }
}